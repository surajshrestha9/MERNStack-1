<html>
    <head>
        <title>MyProfile Page</title>
    </head>
    <body>
        <%- include('common/header'); %>
        <h1>Welcome to My Profile Page</h1>
        <h3>NAME : <%=user.fullname %> </h3>
        <h3>ADDRESS : <%=user.caddress %> </h3>
        <h3>Skills</h3>
        <ul>
            <% user.skills.forEach((item)=>{%>
                <li><%= item %></li>
            <% }); %>
        </ul>
    </body>
</html>